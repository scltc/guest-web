openssl pkcs12 -export -in certificates/fullchain1.pem -inkey certificates/privkey1.pem -out certificates/exhibit.scltc.club.p12 -passout pass:storePassword -name exhibit.scltc.club
rm -f certificates/exhibit.scltc.club.jks
/opt/ejre1.8.0_171/bin/keytool -importkeystore -deststorepass storePassword -destkeystore certificates/exhibit.scltc.club.jks -deststoretype pkcs12 -srckeystore certificates/exhibit.scltc.club.p12 -srcstoretype PKCS12 -srcstorepass storePassword -alias exhibit.scltc.club